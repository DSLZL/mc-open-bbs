{
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=604800, immutable"
                }
            ]
        }
    ],
    "builds": [
        {
            "src": "docs",
            "use": "@vercel/static-build",
            "config": {
                "minify": false
            }
        }
    ]
}
